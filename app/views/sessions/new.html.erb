<h1> Sign In </h1>
    <br>
    <%= form_tag("/signin", method: "post") do %>
    <h2>Your name: </h2>
    <%= collection_select(:inventor, :user_name, Inventor.all, :user_name, :user_name) %><br />
    <h2>Your password: </h2>
    <%= password_field_tag :password%><br><br>
    <%= submit_tag "Sign In"%>

  <% end %>
