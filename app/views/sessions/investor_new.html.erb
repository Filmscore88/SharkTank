<h1> Investor Sign In </h1>
    <br>
    <%= form_tag("/investor_signin", method: "post") do %>
      <h2>Your name: </h2>
      <%= collection_select(:investor, :name, Investor.all, :name, :name) %><br />
      <h2>Your password: </h2>
      <%= password_field_tag :'investor[password]'%><br><br>
      <%= submit_tag "Sign In"%>
    <% end %>
